    <listener class-name="ru.instamart.grpc.listener.GrpcCustomerPaymentsExecutionListener"/>
    <listener class-name="ru.instamart.grpc.listener.GrpcTestSuiteListener"/>
  </listeners>
  <method-selectors>
    <method-selector>
      <selector-class name="ru.instamart.kraken.listener.TestMethodSelector"/>
    </method-selector>
  </method-selectors>
  <test verbose="1" name="Customer Payments">
    <classes>
      <class name="ru.instamart.test.content.customer.PaymentsTest"/>
    </classes>
  </test>
</suite>