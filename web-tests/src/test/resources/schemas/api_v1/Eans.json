{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": [
    "eans",
    "meta"
  ],
  "properties": {
    "eans": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "value",
          "offer_retailer_sku"
        ],
        "properties": {
          "value": {
            "type": "string"
          },
          "offer_retailer_sku": {
            "type": "string"
          }
        }
      }
    },
    "meta": {
      "type": "object",
      "required": [
        "current_page",
        "next_page",
        "total_pages",
        "per_page",
        "total_count"
      ],
      "properties": {
        "current_page": {
          "type": "integer"
        },
        "next_page": {
          "type": ["integer", "null"]
        },
        "total_pages": {
          "type": "integer"
        },
        "per_page": {
          "type": "integer"
        },
        "total_count": {
          "type": "integer"
        }
      }
    }
  }
}
