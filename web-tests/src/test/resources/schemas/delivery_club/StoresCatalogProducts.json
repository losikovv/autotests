{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "totalCount": {
      "type": "integer"
    },
    "data": {
      "type": "object",
      "properties": {
        "products": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "price": {
                "type": "string"
              },
              "discountPrice": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "categories": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "grammar": {
                "type": "integer"
              },
              "weight": {
                "type": "integer"
              },
              "volume": {
                "type": "integer"
              },
              "vat": {
                "type": "integer"
              },
              "imageUrl": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "brand": {
                "type": "string"
              }
            },
            "required": [
              "id",
              "price",
              "discountPrice",
              "name",
              "categories",
              "grammar",
              "weight",
              "volume",
              "vat",
              "imageUrl",
              "brand"
            ]
          }
        }
      },
      "required": [
        "products"
      ]
    }
  },
  "required": [
    "totalCount",
    "data"
  ]
}
