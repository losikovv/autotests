<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Clean up and setup suite" parallel="none">
    <listeners>
        <listener class-name="instamart.core.listeners.ExecutionListenerImpl"/>
    </listeners>
    <test verbose="1" name="REST API tests">
        <groups>
            <run>
                <include name="cleanup"/>
            </run>
        </groups>
        <classes>
            <class name="ru.instamart.tests.Cleanup"/>
        </classes>
    </test> <!-- web-tests -->
</suite> <!-- Custom suite -->
