            <class name="ru.instamart.tests.api.shopper.app.endpoints.ShipmentfulShopperAppTest"/>
            <class name="ru.instamart.tests.api.shopper.app.endpoints.ShipmentlessShopperAppTest"/>
        </classes>
    </test>
</suite>
