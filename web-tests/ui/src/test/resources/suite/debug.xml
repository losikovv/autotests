<!DOCTYPE suite SYSTEM 'http://testng.org/testng-1.0.dtd'>
<suite name="DEBUG-SUITE" verbose="1">
    <listeners>
        <listener class-name="ru.instamart.ui.listener.UiExecutionListener"/>
        <listener class-name="ru.instamart.ui.listener.UiListener"/>
    </listeners>
    <method-selectors>
        <method-selector>
            <selector-class name="ru.instamart.kraken.listener.TestMethodSelector"/>
        </method-selector>
    </method-selectors>
    <test verbose="1" name="Debug">
        <classes>
            <class name="ru.instamart.test.ui.Debug"/>
        </classes>
    </test>
</suite>