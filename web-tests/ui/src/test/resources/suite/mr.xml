<!DOCTYPE suite SYSTEM 'http://testng.org/testng-1.0.dtd'>
<suite name="MR-SUITE" parallel="tests" thread-count="2" verbose="1">
  <listeners>
    <listener class-name="ru.instamart.ui.listener.UiExecutionListener"/>
  </listeners>
  <method-selectors>
    <method-selector>
      <selector-class name="ru.instamart.kraken.listener.TestMethodSelector"/>
    </method-selector>
  </method-selectors>
  <test verbose="1" name="MR UI" parallel="classes" thread-count="4">
    <classes>
      <class name="ru.instamart.test.ui.shopping.ShoppingCatalogTests"/>
      <class name="ru.instamart.test.ui.site.BasicSbermarketTests"/>
      <class name="ru.instamart.test.ui.user.UserRegistrationTests"/>
      <class name="ru.instamart.test.ui.user.UserShippingAddressTests"/>
    </classes>
  </test>
</suite>