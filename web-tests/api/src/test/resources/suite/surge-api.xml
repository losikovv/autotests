<!DOCTYPE suite SYSTEM 'http://testng.org/testng-1.0.dtd'>
<suite name="SURGE-API-SUITE" parallel="tests" thread-count="1" verbose="1">
  <listeners>
    <listener class-name="ru.instamart.api.listener.ApiExecutionListener"/>
    <listener class-name="ru.instamart.api.listener.ApiTestSuiteListener"/>
  </listeners>
  <method-selectors>
    <method-selector>
      <selector-class name="ru.instamart.kraken.listener.TestMethodSelector"/>
    </method-selector>
  </method-selectors>
  <test verbose="1" name="SURGE API" parallel="classes" thread-count="1">
    <classes>
      <class name="ru.instamart.test.api.surge.SurgeTest"/>
    </classes>
  </test>
</suite>