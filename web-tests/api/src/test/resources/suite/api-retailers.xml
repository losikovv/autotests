<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="REST API Suite" data-provider-thread-count="20">
    <listeners>
        <listener class-name="ru.instamart.api.listener.ApiExecutionListener"/>
        <listener class-name="ru.instamart.api.listener.ApiListener"/>
    </listeners>
    <method-selectors>
        <method-selector>
            <selector-class name="ru.instamart.kraken.listener.TestMethodSelector"/>
        </method-selector>
    </method-selectors>
    <test verbose="1" name="API v2 Zones Tests">
        <classes>
            <class name="ru.instamart.test.api.v2.e2e.RetailerE2ETests"/>
        </classes>
    </test>
</suite>
