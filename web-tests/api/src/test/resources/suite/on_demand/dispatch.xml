<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ON-DEMAND Suite">
    <listeners>
        <listener class-name="ru.instamart.api.listener.on_demand.DispatchExecutionListener"/>
        <listener class-name="ru.instamart.api.listener.ApiTestSuiteListener"/>
    </listeners>
    <method-selectors>
        <method-selector>
            <selector-class name="ru.instamart.kraken.listener.TestMethodSelector"/>
        </method-selector>
    </method-selectors>
    <test verbose="1" name="KAFKA">
        <classes>
            <class name="ru.instamart.test.api.on_demand.orderservice.KafkaDispatchTest"/>
            <class name="ru.instamart.test.api.on_demand.orderservice.OrderRedispatchTest"/>
            <class name="ru.instamart.test.api.on_demand.orderservice.OrderServiceKafkaTest"/>
            <class name="ru.instamart.test.api.on_demand.orderservice.OnDemandOrderTest"/>
            <class name="ru.instamart.test.api.on_demand.orderservice.PlanningOrderTest"/>
            <class name="ru.instamart.test.api.on_demand.orderservice.OrderTimeToThrowTest"/>
<!--            <class name="ru.instamart.test.api.on_demand.orderservice.RouteSettingsTest"/> Требует актуализации кейсов-->
            <class name="ru.instamart.test.api.on_demand.orderservice.FullOrderPathAllEnpointsTest"/>
        </classes>
    </test>
</suite>

