<!DOCTYPE suite SYSTEM 'http://testng.org/testng-1.0.dtd'>
<suite name="GRPC-SUITE" verbose="1">
  <listeners>
    <listener class-name="ru.instamart.api.listener.ApiExecutionListener"/>
    <listener class-name="ru.instamart.api.listener.ApiListener"/>
  </listeners>
  <method-selectors>
    <method-selector>
      <selector-class name="ru.instamart.kraken.listener.TestMethodSelector"/>
    </method-selector>
  </method-selectors>
  <test verbose="1" name="PRODUCT HUB">
    <classes>
      <class name="ru.instamart.test.grps.product.hub.ProductHubBackTest"/>
      <class name="ru.instamart.test.grps.product.hub.ProductHubBackStreamTest"/>
      <class name="ru.instamart.test.grps.product.hub.ProductHubFrontDataTest"/>
      <class name="ru.instamart.test.grps.product.hub.ProductHubFrontMetaTest"/>
    </classes>
  </test>
  <test verbose="1" name="PRODUCT FILTER">
    <classes>
      <class name="ru.instamart.test.grps.product.filter.ProductFilterTest"/>
    </classes>
  </test>
</suite>