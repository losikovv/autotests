{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "changedOnFormed": {
      "type": "boolean"
    },
    "expectedDateTime": {
      "type": "string"
    },
    "startedDateTime": {
      "type": "string"
    },
    "deliveredDateTime": {
      "type": ["string","null"]
    },
    "updatedDateTime": {
      "type": ["string","null"]
    },
    "createdDateTime": {
      "type": "string"
    },
    "positions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "originalQuantity": {
            "type": "integer"
          },
          "quantity": {
            "type": "integer"
          },
          "price": {
            "type": "string"
          },
          "discountPrice": {
            "type": "string"
          },
          "totalPrice": {
            "type": "string"
          },
          "discountTotalPrice": {
            "type": "string"
          },
          "updatedDateTime": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "originalQuantity",
          "quantity",
          "price",
          "discountPrice",
          "totalPrice",
          "discountTotalPrice",
          "updatedDateTime"
        ]
      }
    },
    "total": {
      "type": "object",
      "properties": {
        "totalPrice": {
          "type": "string"
        },
        "discountTotalPrice": {
          "type": "string"
        },
        "deliveryPrice": {
          "type": "string"
        }
      },
      "required": [
        "totalPrice",
        "discountTotalPrice",
        "deliveryPrice"
      ]
    }
  },
  "required": [
    "id",
    "status",
    "changedOnFormed",
    "expectedDateTime",
    "startedDateTime",
    "deliveredDateTime",
    "updatedDateTime",
    "createdDateTime",
    "positions",
    "total"
  ]
}
