<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="OnDemand admin tests" data-provider-thread-count="5">
    <listeners>
        <listener class-name="ru.instamart.reforged.core.listener.UiExecutionListener"/>
        <listener class-name="ru.instamart.reforged.core.listener.UiListener"/>
    </listeners>
    <method-selectors>
        <method-selector>
            <selector-class name="ru.instamart.kraken.listener.TestMethodSelector"/>
        </method-selector>
    </method-selectors>
    <test verbose="1" name="Admin OnDemand module" parallel="classes" thread-count="5">
        <classes>
            <class name="ru.instamart.test.reforged.admin.orders.AdministrationBasicOrdersTests"/>
            <class name="ru.instamart.test.reforged.admin.orders.AdministrationShipmentInTableTests"/>
            <class name="ru.instamart.test.reforged.admin.orders.AdministrationMultiselectFiltersOrdersTests"/>

            <class name="ru.instamart.test.reforged.admin.partners.AdminShoppersTests"/>

            <class name="ru.instamart.test.reforged.admin.retailer.AdministrationRetailerRegionsTests"/>
        </classes>
    </test>
</suite>