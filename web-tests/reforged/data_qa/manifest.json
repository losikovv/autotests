{
  "manifest_version": 2,

  "name": "Data QA Plugin",
  "description": "This extension will analyze a page and show rectangle",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["data-qa.js"]
    }
  ],

  "permissions": [
    "activeTab"
  ]
}